{
   "name": "WebService",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "pg": "^8.8.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.10"
   },
   "scripts": {
      "generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./src/migration -d ./src/data-source -p",
      "make-project": "npx typeorm init --name WebService --database postgres --docker",
      "start": "ts-node src/index.ts"
   }
}
